{
  "compilerOptions": {
    "module": "CommonJS",
    "moduleResolution": "node",
    "sourceMap": true,
    "outDir": "dist",
    "strict": true,
    "noImplicitReturns": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "target": "ESNext",
    "noFallthroughCasesInSwitch": true,
    "lib": ["esnext"],
    "esModuleInterop": true
  }
}
